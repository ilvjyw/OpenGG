{
  "dcVipFlag": "true",
  "pCfgJs": "https://javcaptain.com/static/js/playerconfig.js",
  "pCfgJsR": "[\\W|\\S|.]*?MacPlayerConfig.player_list[\\W|\\S|.]*?=([\\W|\\S|.]*?),MacPlayerConfig.downer_list",
  "dcShow2Vip": {},
  "dcPlayUrl": "true",
	"ua": "",
    // 首页地址 用于获取 分类和首页推荐 
	"homeUrl": "https://javcaptain.com/",
    // 手动设置分类，如果手动设置了分类则不使用上面的分类xpath获取分类  例如 "cateManual": {"电影": "1", "电视剧": "2"},
	"cateManual": {"歐美視頻": "29","亞洲無碼": "30","劇情": "28", "主奴": "55","角色扮演": "53","亞洲自拍": "31"},
    // 首页推荐视频的节点
	"homeVodNode": "//ul[contains(@class,'vodlist')]/li[contains(@class,'vodlist')]/a[@title]",
    // 首页推荐视频的名称
	"homeVodName": "/@title",
    // 二次处理正则
    "homeVodNameR": "",
    // 首页推荐视频的id
	"homeVodId": "/@href",
    // 二次处理正则
	"homeVodIdR": "/index.php/vod/detail/id/(\\d+).html",
    // 首页推荐视频的图片
	"homeVodImg": "/@data-original",
    // 二次处理正则
   "homeVodImgR": "\\S+(http\\S+)",
    // 首页推荐视频的简介
	"homeVodMark": "/span[@class='pic_text text_right']/text()",
    // 二次处理正则
    "homeVodMarkR": "",
    // 分类页地址 {cateId} 分类id {catePg} 当前页
	"cateUrl": "https://javcaptain.com/index.php/vod/type/id/{cateId}/page/{catePg}.html",
    // 同上面的homeVod字段 分类列表中的视频信息
	"cateVodNode": "//ul[@class='vodlist vodlist_hb clearfix']/li/a[@title]",
	"cateVodName": "/@title",
	"cateVodId": "/@href",
	"cateVodIdR": "/index.php/vod/detail/id/(\\d+).html",
	"cateVodImg": "/@data-original",
	"cateVodImgR": "\\S+(http\\S+)",
	"cateVodMark": "/span[@class='pic_text text_right']/text()",
    // 详情页地址 用于获取详情页信息 及 播放列表和地址
	"dtUrl": "https://javcaptain.com/index.php/vod/detail/id/{vid}.html",
    // 详情节点
	"dtNode": "//body",
    // 视频名
	"dtName": "//div[@class='pannel_head clearfix']/h2[@class='title']/text()",
	"dtNameR": "",
    // 视频分类
	"dtCate": "//span[contains(text(),'类型')]/following-sibling::*/text()",
	"dtCateR": "",
    // 视频年份
	"dtYear": "//span[contains(text(),'年份')]/following-sibling::*/text()",
	"dtYearR": "",
    // 视频地区
	"dtArea": "//span[contains(text(),'地区')]/following-sibling::*/text()",
	"dtAreaR": "",
    // 视频图片
	"dtImg": "//div[@class='content_thumb fl']/a/@data-original",
	"dtImgR": "",
    // 播放源节点
	"dtFromNode": "//div[@id='NumTab']/a",
    // 播放源名称
	"dtFromName": "/@alt",
    // 二次处理正则
	"dtFromNameR": "",
    // 播放列表节点
	"dtUrlNode": "//ul[@class='content_playlist clearfix']",
    // 播放地址节点
	"dtUrlSubNode": "/li/a",
    // 播放地址
	"dtUrlId": "/@href",
    // 二次处理正则
	"dtUrlIdR": "",
    // 剧集名称
	"dtUrlName": "/text()",
    // 二次处理正则
	"dtUrlNameR": "/index.php/vod/play/id/(\\S+).html",
    // 播放页面的地址 {playUrl} 对应上面 dtUrlId 获取到的地址
	"playUrl": "https://javcaptain.com/index.php/vod/play/id/{playUrl}.html",
    // 解析webview的user-agent
	"playUa": "",
    // 搜索地址
	"searchUrl": "https://javcaptain.com/index.php/vod/search.html?wd={wd}",
    // 同上面的homeVod字段 搜索结果中的视频信息, 这里有对苹果cms suggest搜索接口的特殊支持，参考示例中的极品影视
	"scVodNode": "//ul[contains(@class,'vodlist')]/li[contains(@class,'vodlist')]/a[@title]",
	"scVodName": "/@title",
	"scVodId": "/@href",
	"scVodIdR": "/index.php/vod/detail/id/(\\d+).html",
	"scVodImg": "/@data-original",
	"scVodMark": "/span[@class='pic_text text_right']/text()"
}
