{
	"ua": "",
    // 首页地址 用于获取 分类和首页推荐 
	"homeUrl": "https://www.ttsp.tv/vod/wuyejuchang.html",
    // 分类节点
	"cateNode": "//div[@class='type_list_menu']/ul/li[not(contains(@class,'type_all'))]/a[@title]",
    // 分类名
	"cateName": "/text()",
    // 分类id
	"cateId": "/@href",
    // 分类id二次处理正则
	"cateIdR": "/vodshow/61---(\\S+)--------.html",
    // 手动设置分类，如果手动设置了分类则不使用上面的分类xpath获取分类  例如 "cateManual": {"电影": "1", "电视剧": "2"},
	"cateManual": {},
    // 首页推荐视频的节点
	"homeVodNode": "//ul[contains(@class,'vodlist')]/li[contains(@class,'vodlist_item')]/a",
    // 首页推荐视频的名称
	"homeVodName": "/@title",
    // 二次处理正则
    "homeVodNameR": "",
    // 首页推荐视频的id
	"homeVodId": "/@href",
    // 二次处理正则
	"homeVodIdR": "/voddetail/(\\d+).html",
    // 首页推荐视频的图片
	"homeVodImg": "/@data-original",
    // 二次处理正则
    "homeVodImgR": "",
    // 首页推荐视频的简介
	"homeVodMark": "/span[@class='pic_text text_right']/text()",
    // 二次处理正则
    "homeVodMarkR": "",
    // 分类页地址 {cateId} 分类id {catePg} 当前页
	"cateUrl": "https://www.ttsp.tv/vodshow/61---{cateId}-----{catePg}---.html",
    // 同上面的homeVod字段 分类列表中的视频信息
	"cateVodNode": "//ul[contains(@class,'vodlist')]/li[contains(@class,'vodlist_item')]/a",
	"cateVodName": "/@title",
	"cateVodId": "/@href",
	"cateVodIdR": "/voddetail/(\\d+).html",
	"cateVodImg": "/@data-original",
	"cateVodMark": "/span[@class='pic_text text_right']/text()",
    // 详情页地址 用于获取详情页信息 及 播放列表和地址
	"dtUrl": "https://www.ttsp.tv/voddetail/{vid}.html",
    // 详情节点
	"dtNode": "//body",
    // 视频名
	"dtName": "//span[@class='hd_tit fl']/text()",
	"dtNameR": "",
    // 视频图片
	"dtImg": "//div[contains(@class,'content_box')]/div[contains(@class,'content_thumb')]/a/@data-original",
	"dtImgR": "",
    // 视频分类
	"dtCate": "//ul/li/span[contains(text(),'类型')]//following-sibling::*/text()",
	"dtCateR": "",
    // 视频年份
	"dtYear": "//ul/li/span[contains(text(),'年份')]//following-sibling::*/text()",
	"dtYearR": "",
    // 视频地区
	"dtArea": "//ul/li/span[contains(text(),'地区')]//following-sibling::*/text()",
	"dtAreaR": "",
    // 视频状态
	"dtMark": "//ul/li/span[contains(text(),'状态')]//following-sibling::*/text()",
	"dtMarkR": "",
    // 主演
	"dtActor": "//ul/li/span[contains(text(),'主演')]//following-sibling::*/text()",
	"dtActorR": "",
    // 导演
	"dtDirector": "//ul/li/span[contains(text(),'导演')]//following-sibling::*/text()",
	"dtDirectorR": "",
    // 视频简介
	"dtDesc": "//div[contains(@class,'full_text')/span/text()",
	"dtDescR": "",
    // 播放源节点
	"dtFromNode": "//div[contains(@class,'play_source_tab')]/a",
    // 播放源名称
	"dtFromName": "/@alt",
    // 二次处理正则
	"dtFromNameR": "",
    // 播放列表节点
	"dtUrlNode": "//div[@class='playlist_full']/ul[contains(@class,'content_playlist')]",
    // 播放地址节点
	"dtUrlSubNode": "/li/a",
    // 播放地址
	"dtUrlId": "@href",
    // 二次处理正则
	"dtUrlIdR": "/vodplay/(\\S+).html",
    // 剧集名称
	"dtUrlName": "/text()",
    // 二次处理正则
	"dtUrlNameR": "",
    // 播放页面的地址 {playUrl} 对应上面 dtUrlId 获取到的地址
	"playUrl": "https://www.ttsp.tv/vodplay/{playUrl}.html",
    // 解析webview的user-agent
	"playUa": "",
    // 搜索地址
	"searchUrl": "https://www.ttsp.tv/vodsearch/-------------.html?wd={wd}",
    // 同上面的homeVod字段 搜索结果中的视频信息, 这里有对苹果cms suggest搜索接口的特殊支持，参考示例中的极品影视
	"scVodNode": "//ul[contains(@class,'vodlist')]/li[contains(@class,'vodlist_item')]/",
	"scVodName": "/@title",
	"scVodId": "/@href",
	"scVodIdR": "/voddetail/(\\d+).html",
	"scVodImg": "/@data-original",
	"scVodMark": "/span[@class='pic_text text_right']/text()"
}
